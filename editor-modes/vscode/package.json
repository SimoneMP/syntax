{
    "name": "Mech",
    "displayName": "Mech",
    "description": "Support for the Mech language in Visual Studio Code",
    "version": "0.0.1",
    "publisher": "witheve",
    "icon": "images/logo.png",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Languages"
    ],
    "repository": {
        "url": "https://github.com/mech-lang/syntax.git"
    },
    "contributes": {
        "languages": [
            {
                "id": "Mech",
                "aliases": [
                    "Mech"
                ],
                "extensions": [
                    "mec"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "Mech",
                "scopeName": "mech",
                "path": "./syntaxes/mech.tmLanguage.json"
            }
        ]
    }
}